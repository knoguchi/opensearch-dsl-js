{
  "name": "opensearch-dsl-js",
  "version": "0.1.3",
  "type": "module",
  "description": "Immutable OpenSearch DSL for JavaScript/TypeScript - inspired by Python elasticsearch-dsl",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/knoguchi/opensearch-dsl-js.git"
  },
  "homepage": "https://github.com/knoguchi/opensearch-dsl-js#readme",
  "bugs": {
    "url": "https://github.com/knoguchi/opensearch-dsl-js/issues"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "cli": "node dist/cli.js",
    "install-hooks": "bash scripts/install-hooks.sh",
    "version-from-tag": "bash scripts/version-from-tag.sh",
    "prepublishOnly": "npm run build && npm test",
    "prepack": "npm run build"
  },
  "keywords": [
    "opensearch",
    "elasticsearch",
    "query",
    "dsl",
    "immutable",
    "typescript",
    "query-builder",
    "search",
    "fluent",
    "builder-pattern"
  ],
  "author": "Kenji Noguchi",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node",
    "compilerOptions": {
      "module": "ES2020",
      "moduleResolution": "node"
    }
  }
}
